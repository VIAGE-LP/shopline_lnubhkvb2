<script>
    // 點選左上角logo不跳轉
    $('a[href="/"]').click(function (e) {
        e.preventDefault();
        return;
    });

    // 點選漂浮按鈕
    $('.sizelinkbtn').click(function (e) {
        e.preventDefault();
        let innerheight = window.innerHeight;
        $('html , body').animate({
            // scrollTop: $('#sizelink').offset().top - (0.5 * innerheight),
            scrollTop: $('#sizelink').offset().top - 100,
        }, 900);
    });

    //網址帶有錨點的客人，三秒再推一把
    if (location.hash == "#summer_offer") {
        setTimeout(() => {
            $('html , body').animate({
                scrollTop: $('#summer_offer').offset().top,
            }, 900);
        }, 3000)
    }

    //copy折扣碼 
    function copybtn(couponcode) {
        // 複製（利用 Clipboard API 中的寫入功能）
        navigator.clipboard.writeText(couponcode);
        confirm(`恭喜您成功領取折扣碼！\n您已複製以下優惠代碼 : ${couponcode}\n20分鐘內選擇商品加入購物車後，點選「訂單結帳」，進入結帳畫面\n點選「使用優惠代碼」，並輸入：${couponcode}\n即可享有新客限定優惠 = 網頁價格再折HKD$40(NT$160)`)
    }

    $(".copybtn").on("click", function () {
        copybtn("LUNA09NEW");
    });

</script>

<!-- 20240904開發 任務描述：https://app.asana.com/0/0/1208199719555167/f -->
<script>
    let localCoupon = Date.parse(localStorage.getItem('localCoupon'));

    // 點選 popup 的 popup_img_1(尚未領取)
    $("#popup_img_1").on("click", function () {
        copybtn("LUNA09NEW");
        $("#shopline_popup").hide();
        $("#popup_img_1").hide();

        // 計算 coupon 倒數計時器的開始、結束時間
        const coupon_sec = 1000;
        const coupon_min = coupon_sec * 60;
        const coupon_start_time = new Date();
        const coupon_end_time = new Date(coupon_start_time.getTime() + (coupon_min * 20)); //coupon_start_time 加上 20分鐘 為結束時間

        //將 coupon_end_time 寫入 localstorage，key值命名為 localCoupon
        localStorage.setItem('localCoupon', coupon_end_time);

        $("#coupon_countdown").show();
    });

    // 點選倒數計時 coupon icon
    $("#coupon_countdown").on("click", function () {
        $("#shopline_popup").show();
        $("#popup_img_2").show();
    });

    // 點選 popup 的 popup_img_2(尚未使用)
    $("#popup_img_2").on("click", function () {
        copybtn("LUNA09NEW");
        $("#shopline_popup").hide();
    });

    let now_time = new Date();
    let scroll_first = true;

    //判斷 localStorage 中的 localCoupon 是否已經有值
    if (!localCoupon) {
        window.onscroll = function () {
            if (scroll_first) {
                $("#shopline_popup").show();
                $("#popup_img_1").show();
            }
            scroll_first = false;
        };

        //每1秒刷新一次
        setInterval(() => {
            now_time = new Date();
            localCoupon = Date.parse(localStorage.getItem('localCoupon'));
            const differ_end = localCoupon - now_time.getTime();
            //判斷刷新後的now_time，是否還在期間內
            if (differ_end > 0) {
                coupon_timer(differ_end);
            } else {
                $("#coupon_countdown").hide();
            }
        }, 1000);

    } else {
        if (now_time < localCoupon) {
            $("#coupon_countdown").show();

            //每1秒刷新一次
            setInterval(() => {
                now_time = new Date();
                const differ_end = localCoupon - now_time.getTime();
                //判斷刷新後的now_time，是否還在期間內
                if (differ_end > 0) {
                    coupon_timer(differ_end);
                } else {
                    $("#coupon_countdown").hide();
                }
            }, 1000);
        } else {
            window.onscroll = function () {
                if (scroll_first) {
                    $("#shopline_popup").show();
                    $("#popup_img_1").show();
                }
                scroll_first = false;
            };

            //每1秒刷新一次
            setInterval(() => {
                now_time = new Date();
                localCoupon = Date.parse(localStorage.getItem('localCoupon'));
                const differ_end = localCoupon - now_time.getTime();
                //判斷刷新後的now_time，是否還在期間內
                if (differ_end > 0) {
                    coupon_timer(differ_end);
                } else {
                    $("#coupon_countdown").hide();
                }
            }, 1000);
        }
    }

    // coupon 倒數計時器專用 計時函式
    function coupon_timer(differ_end) {
        const sec = Math.floor(differ_end / 1000 % 60);
        const min = Math.floor(differ_end / 1000 / 60 % 60);

        document.querySelector('#coupon_countdown .coupon_seconds_number').textContent = String(sec).padStart(2, '0');
        document.querySelector('#coupon_countdown .coupon_minutes_number').textContent = String(min).padStart(2, '0');
    }
</script>